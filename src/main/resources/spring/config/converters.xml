		<bean class="converters.DanceSchoolToStringConverter" />
		<bean class="converters.DanceTestToStringConverter" />
		<bean class="converters.EventToStringConverter" />
		<bean class="converters.ManagerToStringConverter" />
		<bean class="converters.MaterialToStringConverter" />
		<bean class="converters.MessageToStringConverter" />
		<bean class="converters.PatnerInvitationToStringConverter" />
